{
  "name": "superpowers-game-fmouseinput-plugin",
  "version": "0.2.0",
  "description": "Allows for easy setup of interactions between the mouse and actors in the Superpowers Game system for Superpowers, the HTML5 2D+3D game maker.",
  "repository": {
    "type": "git",
    "url": "https://github.com/florentpoujol/superpowers-game-fmouseinput-plugin"
  },
  "author": "Florent Poujol <florent.poujol@gmail.com> (http://florentpoujol.fr)",
  "dependencies": {

  },
  "devDependencies": {
    "gulp": "3.9.x",
    "typescript": "1.6.x",
    "browserify": "10.2.x"
  },
  "keywords": [
    "superpowers",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/florentpoujol/superpowers-game-fmouseinput-plugin/issues"
  },
  "scripts": {
    "compilets": "tsc api/ff.ts --out gitignore/ff.js --module commonjs --target ES5",
    "gendoc": "typedoc --out public/docs api/main.ts --mode file --module commonjs --target ES5 --name \"fMouseInput component plugin\" --theme minimal --readme tutorial.md",
    
    "linkplugindev": "mklink /j \"../../app/systems/supGame/plugins/florentpoujol/fMouseInput\" \"./\"",
    "linkpluginrelease": "mklink /j \"../../../release/app/systems/supGame/plugins/florentpoujol/fMouseInput\" \"./\"",

    "linkprojectdev": "mklink /j \"../../data/projects/fMouseInput\" \"./project/fMouseInput\"",
    "linkprojectrelease": "mklink /j \"../../../localprojects/fMouseInput\" \"./project/fMouseInput\"",

    "build": "gulp --gulpfile=../../../../../scripts/pluginGulpfile.js --cwd=."
  },
  "maintainers": [
    "Florent Poujol <florent.poujol@gmail.com>"
  ],
  "homepage": "https://github.com/florentpoujol/superpowers-game-fmouseinput-plugin",
  "license": "MIT"
}
